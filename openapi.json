{
  "openapi": "3.1.0",
  "info": {
    "title": "PrizePicks Data API",
    "description": "Provides current PrizePicks prop bet data scraped daily",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "https://raw.githubusercontent.com/ENKDAY/prizepicks-scraper/main"
    }
  ],
  "paths": {
    "/data/prizepicks.json": {
      "get": {
        "summary": "Get ALL standard PrizePicks props",
        "description": "Returns all available standard-odds prop bets across supported sports (no goblin/demon variants). Use sport-specific endpoints for smaller payloads.",
        "operationId": "getPrizePicks",
        "responses": {
          "200": {
            "description": "Successful response with prop bet data",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/PrizePicksData"
                }
              }
            }
          }
        }
      }
    },
    "/data/prizepicks-ncaaf.json": {
      "get": {
        "summary": "Get ALL standard College Football props",
        "description": "Returns ALL College Football prop bets (standard odds only; excludes goblin/demon variants).",
        "operationId": "getNCAAFPropsAll",
        "responses": {
          "200": {
            "description": "Successful response with all CFB props",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/PrizePicksData"
                }
              }
            }
          }
        }
      }
    },
    "/data/prizepicks-ncaaf-qb.json": {
      "get": {
        "summary": "Get College Football QB props only (standard)",
        "description": "Returns only QB-related props (passing yards, passing TDs, completions, etc.) for faster loading. Standard odds only.",
        "operationId": "getNCAAFQBProps",
        "responses": {
          "200": {
            "description": "Successful response with CFB QB prop bet data",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/PrizePicksData"
                }
              }
            }
          }
        }
      }
    },
    "/data/prizepicks-ncaaf-rb.json": {
      "get": {
        "summary": "Get College Football RB props only (standard)",
        "description": "Returns only RB-related props (rushing yards, rushing TDs, etc.). Standard odds only.",
        "operationId": "getNCAAFRBProps",
        "responses": {
          "200": {
            "description": "Successful response with CFB RB prop bet data",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/PrizePicksData"
                }
              }
            }
          }
        }
      }
    },
    "/data/prizepicks-ncaaf-wr.json": {
      "get": {
        "summary": "Get College Football WR props only (standard)",
        "description": "Returns only WR-related props (receiving yards, receptions, receiving TDs, etc.). Standard odds only.",
        "operationId": "getNCAAFWRProps",
        "responses": {
          "200": {
            "description": "Successful response with CFB WR prop bet data",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/PrizePicksData"
                }
              }
            }
          }
        }
      }
    },
    "/data/prizepicks-nfl.json": {
      "get": {
        "summary": "Get NFL props (standard only)",
        "description": "Returns only NFL prop bets (standard odds).",
        "operationId": "getNFLProps",
        "responses": {
          "200": {
            "description": "Successful response with NFL prop bet data",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/PrizePicksData"
                }
              }
            }
          }
        }
      }
    },
    "/data/prizepicks-nba-today.json": {
      "get": {
        "summary": "Get today's NBA props (standard)",
        "description": "Returns only today's NBA prop bets (standard odds) to keep payload small.",
        "operationId": "getNBAPropsToday",
        "responses": {
          "200": {
            "description": "Successful response with NBA prop bet data for today",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/PrizePicksData"
                }
              }
            }
          }
        }
      }
    },
    "/data/prizepicks-nba-tomorrow.json": {
      "get": {
        "summary": "Get NBA props for tomorrow only (standard)",
        "description": "Returns only tomorrow's NBA prop bets (standard odds) to keep payload small.",
        "operationId": "getNBAPropsTomorrow",
        "responses": {
          "200": {
            "description": "Successful response with NBA prop bet data for tomorrow",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/PrizePicksData"
                }
              }
            }
          }
        }
      }
    },
    "/data/nba-today/{team}.json": {
      "get": {
        "summary": "Get today's NBA props for a specific team (standard)",
        "description": "Returns only today's NBA prop bets for the given team (standard odds). Team parameter is a slug of the team name (market + mascot), lowercased and hyphenated (e.g., boston-celtics).",
        "operationId": "getNBAPropsTodayByTeam",
        "parameters": [
          {
            "name": "team",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response with NBA prop bet data for the team",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/PrizePicksData"
                }
              }
            }
          }
        }
      }
    },
    "/data/nba-tomorrow/{team}.json": {
      "get": {
        "summary": "Get tomorrow's NBA props for a specific team (standard)",
        "description": "Returns only tomorrow's NBA prop bets for the given team (standard odds). Team parameter is a slug of the team name (market + mascot), lowercased and hyphenated.",
        "operationId": "getNBAPropsTomorrowByTeam",
        "parameters": [
          {
            "name": "team",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response with NBA prop bet data for the team",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/PrizePicksData"
                }
              }
            }
          }
        }
      }
    },
    "/data/nfl-today/{team}.json": {
      "get": {
        "summary": "Get today's NFL props for a specific team (standard)",
        "description": "Returns only today's NFL prop bets for the given team (standard odds). Team parameter is a slug of the team name (market + mascot), lowercased and hyphenated.",
        "operationId": "getNFLPropsTodayByTeam",
        "parameters": [
          {
            "name": "team",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response with NFL prop bet data for the team",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/PrizePicksData"
                }
              }
            }
          }
        }
      }
    },
    "/data/nfl-tomorrow/{team}.json": {
      "get": {
        "summary": "Get tomorrow's NFL props for a specific team (standard)",
        "description": "Returns only tomorrow's NFL prop bets for the given team (standard odds). Team parameter is a slug of the team name (market + mascot), lowercased and hyphenated.",
        "operationId": "getNFLPropsTomorrowByTeam",
        "parameters": [
          {
            "name": "team",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response with NFL prop bet data for the team",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/PrizePicksData"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "PrizePicksData": {
        "type": "object",
        "properties": {
          "scrapedAt": {
            "type": "string",
            "format": "date-time",
            "description": "ISO 8601 timestamp of when data was scraped"
          },
          "scrapedDate": {
            "type": "string",
            "description": "Human-readable date of scraping"
          },
          "source": {
            "type": "string",
            "description": "Data source (PrizePicks)"
          },
          "totalProps": {
            "type": "integer",
            "description": "Total number of prop bets available"
          },
          "props": {
            "type": "array",
            "description": "Array of all prop bets",
            "items": {
              "$ref": "#/components/schemas/PropBet"
            }
          }
        }
      },
      "PropBet": {
        "type": "object",
        "properties": {
          "player": {
            "type": "string",
            "description": "Player name"
          },
          "Team": {
            "type": "string",
            "description": "Team name (market + mascot)"
          },
          "stat": {
            "type": "string",
            "description": "Stat type (Points, Rebounds, Passing Yards, etc.)"
          },
          "line": {
            "type": "number",
            "description": "Over/under line value"
          },
          "sport": {
            "type": "string",
            "description": "Sport (NBA, NFL, MLB, etc.)"
          },
          "Opponent": {
            "type": "string",
            "description": "Opponent name (market + mascot)"
          },
          "teamCode": {
            "type": "string",
            "description": "Team abbreviation/code when available"
          },
          "opponentCode": {
            "type": "string",
            "description": "Opponent abbreviation/code when available"
          },
          "startTime": {
            "type": "string",
            "description": "Game start time in CST formatted as MM/DD/YY HH:MM AM/PM CST"
          },
          "oddsType": {
            "type": "string",
            "description": "Odds type (standard only in this dataset)"
          },
          "gameId": {
            "type": "string",
            "description": "Game identifier from PrizePicks"
          },
          "rank": {
            "type": "integer",
            "description": "PrizePicks-provided rank/order within the board"
          }
        }
      }
    }
  }
}
